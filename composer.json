{
    "name": "geodiv/geodiv",
    "type": "project",
    "repositories": [{
        "type": "composer",
        "url": "https://composer-spip.lerebooteux.fr"
    }],
    "require": {
        "php": "^5.3 <7.2",
        "spip/composer-installer": "^0.2",
        "spip/cms": "3.1.*",
        "spip/mots": "2.7.*",
        "spip/organiseur": "1.0.*",
        "spip/images": "1.2.*",
        "geodiv/squelettes": "@dev",
        "spipremix/zcore": "^2.6",
        "spipremix/polyhierarchy": "^3.6",
        "spipremix/bonux": "^3.4",
        "spipremix/date_inscription": "^0.2.1",
        "spipremix/nuage": "^4.0",
        "spip/jquery_ui": "1.11.*"
    },
    "license": "GPL-3.0-or-later",
    "authors": [{
        "name": "Bruno Bergot",
        "email": "bruno@eliaz.fr"
    }],
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "5.3.29"
        }
    },
    "extra": {
        "spip": {
            "template": "geodiv/squelettes",
            "extensions": [
                "spip/mots",
                "spip/organiseur",
                "spip/images",
                "spipremix/zcore",
                "spipremix/polyhierarchy",
                "spipremix/bonux",
                "spip/dist",
                "spip/breves",
                "spip/sites",
                "spip/medias",
                "spip/forum",
                "spipremix/date_inscription",
                "spipremix/nuage",
                "spip/jquery_ui"
            ]
        }
    }
}
